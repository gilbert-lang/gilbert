[config]
jobmanager=node1.stsffap.org
template = evaluation/src/main/resources/matrixMult.gb.template
engine = Stratosphere
blocksize = 10
compilerHints = true
optimization.TP = true
optimization.MMReordering = true
densityThreshold = 0.6
output = matrixMult
tries = 10
parallelism = 4
outputPath = ""
checkpointDir = ""
iterationUntilCheckpoint = 0
math = Breeze

[data]
rowsA = 1000, 10000, 100000
colsA = 1000, 10000, 100000
colsB = 1000, 10000, 100000
level = 0.5

[header]
header = RowsA:rowsA ColsA:colsA ColsB:colsB Time:time Error:error
