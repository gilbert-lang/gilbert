[config]
master=flink-till-m
template = matrixMult.gb.template
engine = Spark
blocksize = 500
compilerHints = true
optimization.TP = true
optimization.MMReordering = true
densityThreshold = 0.5
outputFile = matrixMult/matrixMultScalabilitySpark
tries = 2
parallelism = 1, 2, 4, 8, 16, 32, 64, 128
outputPath = file://tmp/spark
checkpointDir =
iterationUntilCheckpoint = 0
math = Breeze
memory = 47g

[data]
rowsA = 7500, 9500, 12000, 15000, 19000, 30000, 37000
colsA = 7500, 9500, 12000, 15000, 19000, 30000, 37000
colsB = 7500, 9500, 12000, 15000, 19000, 30000, 37000
level = 0.1

[header]
header = RowsA:rowsA ColsA:colsA ColsB:colsB Time:time Error:error
